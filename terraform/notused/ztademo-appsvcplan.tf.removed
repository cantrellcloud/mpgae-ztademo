module "it_production_eastus_cces_appsvcplan" {
	source = "github.com/cantrellcloud/tfAzureModules/appsvcplan"
	#to_provision = local.provision_appsvcplan01
	rg_location = module.it_production_eastus_cces_appsvcplan_rg.rg_location
	rg_name = module.it_production_eastus_cces_appsvcplan_rg.rg_name

	appsvcplan_name = "it_production_eastus_cces_appsvcplan"
    kind = "Linux"
    reserved = true
    is_xenon = false

    #sku
        sku_tier = "Basic"
        sku_size = "B2"
        #sku_capacity = 4
}

   #appsvcplan01_outputs
   output "it_production_eastus_cces_appsvcplan_id" {
        value = module.it_production_eastus_cces_appsvcplan.appsvcplan_id
    }
    output "it_production_eastus_cces_appsvcplan_name" {
        value = module.it_production_eastus_cces_appsvcplan.appsvcplan_name
    }
    output "it_production_eastus_cces_appsvcplan_kind" {
        value = module.it_production_eastus_cces_appsvcplan.appsvcplan_kind
    }
    output "it_production_eastus_cces_appsvcplan_appsvcplan_maximum_number_of_workers" {
        value = module.it_production_eastus_cces_appsvcplan.appsvcplan_maximum_number_of_workers
    }

